{
  "private": true,
  "scripts": {
    "dev": "run-s clean && cross-env SOCKET=true node ./scripts/next-watch.js ../contents",
    "start": "run-s clean && node ./scripts/server.js",
    "build": "run-s clean && next build",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "export:next": "next export",
    "export:posts-assets": "node scripts/copy-assets.js",
    "export": "run-s build export:next export:posts-assets",
    "format": "prettier --config .prettierrc '**/*.+(ts|tsx)' --write",
    "test": "run-p typecheck test:ci export --print-label && run-s clean:all",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "coveralls": "npm run test:coverage && coveralls < ./coverage/lcov.info",
    "clean": "rm -rf .tmp",
    "clean:all": "run-s clean && rm -rf .next && rm -rf out && rm -rf build",
    "typecheck": "tsc",
    "prepare:dev": "run-s clean:all build:content-service",
    "build:content-service": "tsc --project ./scripts/tsconfig.build.json && ncc build -m ./build/libs/content-service -o scripts/libs/content-service"
  },
  "dependencies": {
    "classnames": "2.3.1",
    "date-fns": "2.28.0",
    "excerpt-html": "^1.2.2",
    "glob": "^8.0.1",
    "gray-matter": "4.0.3",
    "lodash": "^4.14.182",
    "next": "^12.1.5",
    "next-themes": "^0.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "remark": "13.0.0",
    "remark-gfm": "1.0.0",
    "remark-html": "^13.0.2",
    "remark-prism": "^1.3.6",
    "unist-util-visit": "^2.0.3",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.5",
    "@types/remark-prism": "^1.3.3",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@types/url-join": "^4.0.1",
    "@vercel/ncc": "^0.33.4",
    "autoprefixer": "^10.4.2",
    "chalk": "^2.4.2",
    "chokidar": "^3.5.3",
    "commander": "^9.2.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "express": "^4.17.3",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "prettier": "^2.6.2",
    "recursive-copy": "^2.0.14",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16"
  }
}
